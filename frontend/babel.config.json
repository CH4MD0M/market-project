{
  "presets": [
    ["@babel/preset-env", { "targets": { "browsers": ["last 2 versions", "safari >= 7"] } }],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    ["@babel/preset-typescript", { "isTSX": true, "allExtensions": true }]
  ],
  "plugins": [["@babel/plugin-transform-runtime", { "corejs": 3 }]]
}
